            <font color="<%=info.colorS%>">üõ†Ô∏è Setup Settings
            </font>
        </a>

    </div>
    <div class="sidenav">
        <table>
            <% if(user && user.id){ %>

                <% user.guilds.filter(guild=>{
                    const permsOnGuild = new Permissions(guild.permissions);
                    if(permsOnGuild.has("MANAGE_GUILD")) return guild;

                    }).sort((a, b) => {
                    return (bot.guilds.cache.get(a.id) == bot.guilds.cache.get(b.id)) ? 0 : a ? -1 : 1;
                    }).forEach(guild => {
                    %>
                    <% if(bot.guilds.cache.get(guild.id)){ %>

                        <a href="<%- botconfig.domain %>/dashboard/<%- guild.id %>">
                            <img src="https://cdn.discordapp.com/icons/<%=guild.id%>/<%=guild.icon%>.png" width="100%"
                                class="Gicon">
                        </a>

                        <% }else { %>

                            <a href="<%- botconfig.domain %>/dashboard/<%- guild.id %>">
                                <img src="https://cdn.discordapp.com/icons/<%=guild.id%>/<%=guild.icon%>.png"
                                    width="100%" class="GiconBlack">
                                <% } %>
                                    <% }) %>
                                        <% }%>
